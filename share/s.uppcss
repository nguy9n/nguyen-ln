* {
	font-family: Outfit, sans-serif;
}

body {
	text-align: justify;
	max-width: 600px;
	margin: auto;
	padding: 20px;
	background-image: url(trees.png);
	color: #fff;
}

nav {
	background-color: rgba(84, 82, 18, 0.5);
	font-size: 1.5em;
}

nav a {
	color: #fff;
	background-color: rgba(36, 85, 18, 0.5);
	text-align: center;
}

a:hover {
	font-weight: bold;
}

p {
	background-color: rgba(84, 82, 18, 0.5);
}

