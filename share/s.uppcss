* {
	font-family: Outfit, sans-serif;
	margin: auto;
	max-width: 600px;
	padding: 10px;
}

body {
	text-align: justify;
	background-image: url(trees.png);
	color: #fff;
}

nav {
	background-color: rgba(84, 82, 18);
	font-size: 1.1em;
	text-align: center;
}

nav a {
	color: #fff;
	background-color: rgba(23, 55, 12, 0.5);
	text-decoration: none;
}

a:hover {
	font-weight: bold;
}

p {
	background-color: rgba(84, 82, 18, 0.5);
}

