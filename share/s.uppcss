* {
	font-family: Outfit, sans-serif;
}

body {
	text-align: justify;
	max-width: 500px;
	margin: auto;
	padding: 10px;
	background-image: url(trees.png);
	color: #fff;
}

nav {
	background-color: rgb(128, 135, 23);
	font-size: 2em;
}

nav a {
	color: #fff;
	background-color: rgb(35, 135, 23);
}

a:hover {
	font-weight: bold;
}

p {
	background-color: rgba(128, 135, 23, 0.5);
}

